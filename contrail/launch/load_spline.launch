<?xml version='1.0'?>
<launch>
	<arg name="s_name" default=""/>

	<node pkg="contrail" type="load_spline" name="load_spline" clear_params="true" output="screen">
		<param name="frame_id" value="map" />

		<rosparam command="load" file="$(find contrail)/splines/$(arg s_name).yaml"/>
		<remap from="spline" to="emulated_uav/guidance/reference/contrail/spline"/>
	</node>
</launch>
